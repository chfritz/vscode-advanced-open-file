{
    "name": "vscode-advanced-open-file",
    "displayName": "vscode-advanced-open-file",
    "description": "",
    "version": "0.1.8",
    "publisher": "jit-y",
    "repository": {
        "type": "git",
        "url": "https://github.com/jit-y/vscode-advanced-open-file"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.advancedOpenFile"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.advancedOpenFile",
                "title": "Advanced Open File"
            }
        ],
        "keybindings": [
            {
                "command": "extension.advancedOpenFile",
                "key": "ctrl+x ctrl+f"
            }
        ],
        "configuration": {
            "title": "VscodeAdvancedOpenFile Configuration",
            "properties": {
                "vscode-advanced-open-file.selectPath": {
                    "type": "boolean",
                    "default": true,
                    "description": "Select the path when QuickPick opens"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "yarn run compile && node ./node_modules/vscode/bin/test",
        "fmt": "prettier --write 'src/**/*.ts'",
        "prettier-check": "prettier --check 'src/**/*.ts'",
        "tslint-check": "tslint-config-prettier-check ./tslint.json"
    },
    "devDependencies": {
        "@types/mkdirp": "^0.5.2",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.10.2",
        "prettier": "^1.16.1",
        "tslint": "^5.12.1",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.2.4",
        "vscode": "^1.1.26"
    },
    "dependencies": {
        "mkdirp": "^0.5.1"
    }
}
